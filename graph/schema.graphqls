type Category {
  id: ID!
  name: String!
  description: String
  courses: [Course!]!
}

type Category {
  id: ID!
  name: String!
  description: String
  category: Category!
}

input NewCategory {
  name: String!
  description: description
}

input NewCourse {
  name: String!
  description: description
  categoryId: ID!
}

type Query {
  categories: [Category!]!
  courses: [Course!]!
}

type Mutation{
  createCategory(input: NewCategory!): Category!
  cresteCourse(input: NewCourse!): Course!
}